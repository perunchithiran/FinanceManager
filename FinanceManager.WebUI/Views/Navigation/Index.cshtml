@model IEnumerable<string>


@foreach (var link in Model)
{
    @Html.RouteLink(link, new
    {
        controller = ViewBag.controllerName,
        action = "Index",
        date = link
    }, new
    {
        @class = "btn btn-block" + (link == ViewBag.selectedDate ? " btn-primary" : " btn-outline-primary")
    })
    <br />
}