@model IEnumerable<string>


@foreach (var link in Model)
{
    @Html.RouteLink(link, new
    {
        controller = ViewBag.type,
        action = (ViewBag.type == "Summary" ? "Show" : "Index"),
        date = link
    }, new
    {
        @class = "btn btn-block" + (link == ViewBag.selectedDate ? " btn-primary" : " btn-outline-primary")
    })
    <br />
}