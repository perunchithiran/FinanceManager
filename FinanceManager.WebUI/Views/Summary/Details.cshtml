@model FinanceManager.Domain.Entities.Details
@{
    ViewBag.Title = "Szczegółowa analiza" + Model.Coniugation;
    Layout = null;
}

    <div><h4>Analiza @Model.Coniugation wg kategorii ( @Model.CategorySum.ToString("c") )</h4></div>
    <ul>
        @foreach (var item in Model.DetailedList)
        {
            <li>
                @item.Item1:  @item.Item2.ToString("c"), co stanowi <b>@item.Item3</b> % @Model.Coniugation
            </li>

        }
    </ul>